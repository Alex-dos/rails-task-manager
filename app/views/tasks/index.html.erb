<h1>Task Manager</h1>

<ul>
  <% @tasks.each do |task| %>
  <li>
    <% if task.completed == true %>
  <i class="far fa-check-square"></i>
<% else %>
  <i class="far fa-square"></i>
<%end%>
    <%= link_to task.title, task_path(task) %>
  <% end %>
  </li>
</ul>


<%= link_to "Create Task", tasks_new_path %>
